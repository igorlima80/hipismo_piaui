<p id="notice"><%= notice %></p>

<h1>Class Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Class plan</th>
      <th>School day</th>
      <th>Registration</th>
      <th>Animal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @class_rooms.each do |class_room| %>
      <tr>
        <td><%= class_room.start_time %></td>
        <td><%= class_room.end_time %></td>
        <td><%= class_room.class_plan_id %></td>
        <td><%= class_room.school_day_id %></td>
        <td><%= class_room.registration_id %></td>
        <td><%= class_room.animal_id %></td>
        <td><%= link_to 'Show', class_room %></td>
        <td><%= link_to 'Edit', edit_class_room_path(class_room) %></td>
        <td><%= link_to 'Destroy', class_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Class Room', new_class_room_path %>
